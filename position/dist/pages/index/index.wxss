/**index.wxss**/

.welcome{
  position: fixed;
  width: 100%;
  height: 100%;
  z-index: 100;
  background-color:rgba(0,0,0,.5);
}
.come{
  position: relative; 
  width: 75%;
  height: 230rpx;
  left: 0;
  right: 0;
  top: 50%;
  bottom: 0;
  margin: auto;
  margin-top:-120rpx;
  padding: 70rpx 0;
  z-index: 9999;
  background-color: #fff;
  border-radius: 30rpx;
}
.welcome_title{
  text-align: center;
  font-size: 40rpx;
}
.welcome_content{
  padding: 30rpx 0;
  text-align: center;
  color: #bababa;
  border-bottom: 1px solid #dfd9d9;
}
.come>button{
  font-size: 40rpx;
  color:#fff;
  background-color: #fff;
}
.come>button::after{
  border:none;
}
#myMap{
  width:100%;
  height:1000rpx;
}
.userLicenseAgreementBox,.repairFeedbackBox{
  position: fixed;
  width: 100%;
  height: 100%;
  background-color:rgba(0,0,0,.5);
  display: flex;
  align-items: center;
  justify-content: center;
}
.userLicenseAgreement{
  position: relative;
  width: 256px;
  height: 230px;
  background-color: #fff;
  color:#ccc;
  /* padding:20px; */
  border-radius: 15rpx;
}
.userLicenseAgreementTitle{
  color:#000;
  margin:20px 0;
  text-align: center;
  font-size: 20px;
}
.userLicenseAgreementExplain{
  color:rgb(153,153,153);
  text-indent: 2rem;
  padding:0 20px;
}
.userLicenseAgreementLink{
  margin: 15px 0;
  color: rgb(132,157,293);
  text-align: center;
  font-size: 12px;
}
.userLicenseAgreementBtn{
  padding: 10px 0;
  color:rgb(9,187,7);
  font-size: 20px;
  text-align: center;
}
.repair{
  display: flex;
  flex-direction:row-reverse;
  position: relative;
  width:100%;
  height:20px;
  right: 20px;
  padding-bottom:10px;
}
.repair image{
  width: 20px;
  height: 20px;
}
checkbox-group label{
  display: block;
  padding:10px 0;
}
.bottom-btn{
  position: fixed;
  bottom:0;
  left: 30%;
}
.contact-btn{
  background:blue;
  width: 100px;
  height: 30px;
}
.bot-btn{
  background:yellow;
  width: 100px;
  height: 30px;
}